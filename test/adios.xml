<?xml version="1.0"?>
<adios-config>
    <io name="writerIO">
        <engine type="BP5">
            <parameter key="OpenTimeoutSecs" value="60.0"/>
        </engine>
        <variable name="IMAG">
            <operation type="mgard">
                <parameter key="accuracy" value="0.1"/>
                <parameter key="mode" value="ABS"/>
                <parameter key="lossless" value="Huffman_Zstd"/>
            </operation>
            <!-- <operation type="sz">
                <parameter key="accuracy" value="0.001"/>
            </operation> -->
        </variable>
    </io>

</adios-config>
